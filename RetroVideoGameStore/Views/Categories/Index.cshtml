@model IEnumerable<RetroVideoGameStore.Models.Category>
@{
    ViewData["Title"] = "Categories";
}

<h1>Categories</h1>
<h2>Pick a category to browse our store</h2>
<ul>
    @foreach (var category in Model)
    {
        <li><a asp-controler="catefories" asp-action="Browse" asp-route-categoryName="@category.CategoryName">@category</a></li>
    }
    <li><a asp-controller="Categories" asp-action="Browse" asp-route-categoryName="Category 1">Category 1</a></li>
    <li><a asp-controller="Categories" asp-action="Browse" asp-route-categoryName="Category 2">Category 2</a></li>
    <li><a asp-controller="Categories" asp-action="Browse" asp-route-categoryName="Category 3">Category 3</a></li>
</ul>